{
  "name": "@watergis/mbtiles2pbf",
  "version": "0.2.1",
  "description": "This module will extract pbf files from mbtile by using mbutil.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "es6/index.js",
  "files": [
    "dist",
    "es6"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist",
    "build:esm": "tsc --project . --module es2015 --outDir ./es6",
    "precommit": "eslint --fix 'src/**/*.ts' && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watergis/mbtiles2pbf.git"
  },
  "author": "Jin IGARASHI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/watergis/mbtiles2pbf/issues"
  },
  "homepage": "https://github.com/watergis/mbtiles2pbf#readme",
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/es6-promise": "^3.3.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.23",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "better-sqlite3": "^7.1.0",
    "fs-extra": "^10.0.0",
    "rimraf": "^3.0.2",
    "zlib": "^1.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^#/(.+)": "<rootDir>/src/$1"
    }
  }
}
